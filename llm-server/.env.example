# Database Configuration
DB_PASSWORD=your_secure_database_password

# Cloud Langfuse Configuration
YOUR_LANGFUSE_CLOUD_HOST=https://cloud.langfuse.com
YOUR_LANGFUSE_PUBLIC_KEY=pk-lf-your-public-key
YOUR_LANGFUSE_SECRET_KEY=sk-lf-your-secret-key

# API Configuration
YOUR_OPENAI_API_KEY=sk-your-openai-api-key
YOUR_ANTHROPIC_API_KEY=sk-ant-your-anthropic-api-key
YOUR_TAVILY_API_KEY=tvly-your-tavily-api-key

# Neo4j Configuration
YOUR_NEO4J_URI=neo4j+s://your-neo4j-instance.databases.neo4j.io
YOUR_NEO4J_USERNAME=neo4j
YOUR_NEO4J_PASSWORD=your-neo4j-password

# MongoDB Configuration
YOUR_MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/database

# KIS API Configuration (Korean Investment & Securities)
YOUR_KIS_APP_KEY=your-kis-app-key
YOUR_KIS_APP_SECRET=your-kis-app-secret
YOUR_KIS_ACCESS_TOKEN=your-kis-access-token

# Server Configuration
REQUESTS_TIMEOUT=300
SERVER_PORT=8000
