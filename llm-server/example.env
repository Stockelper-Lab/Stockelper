# LLM API Keys
OPENAI_API_KEY=your_openai_api_key_here

# Database Connection Settings
MILVUS_URI=http://localhost:21001
ASYNC_DATABASE_URL=postgresql+asyncpg://user:password@localhost:5432/stockelper
CHECKPOINT_DATABASE_URI=postgresql://user:password@localhost:5432/stockelper

# Langfuse Monitoring
LANGFUSE_HOST=http://langfuse-server:3000
LANGFUSE_PUBLIC_KEY=pk-lf-your-langfuse-public-key
LANGFUSE_SECRET_KEY=sk-lf-your-langfuse-secret-key

# External API Keys
